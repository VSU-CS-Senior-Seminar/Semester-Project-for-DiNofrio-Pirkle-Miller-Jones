<h1>Newsfeed page</h1>
<table id="Newsfeed", class="table table-hover">
    <thead>
    <tr>
      <th>Category</th>
      <th>Title</th>
      <th>Content</th>
      <th>Likes</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody> 
    <% @posts.each do |p| %>
    <tr>
        <td><%= p.category %></td>
        <td><%= p.title %></td>
        <td><%= p.content %></td>
        <td><%= p.likes %></td>
    </tr>
    <% end %>
  </tbody>
</table>
