<h1>Newsfeed page</h1>
<table id="Newsfeed">
    <thead>
    <tr>
      <th>Category</th>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody> 
    <tr>
      <!--<td>Some Category</td>-->
      <!--<td>Some Title</td>-->
      <!--<td>The Content of the post</td>-->
    </tr>
    <% @posts.each do |post| %>
    <tr>
        <td><%= post.category %></td>
        <td><%= post.title %></td>
        <td><%= post.content %></td>
    </tr>
    <% end %>
  </tbody>
</table>
