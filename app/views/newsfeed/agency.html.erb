</div>
  
  <div class="container1">
      <table id="Newsfeed", class="table table-hover">
        <thead>
              <% @posts.each do |p| %>
                <% if User.find(p.user_id).zipcode.eql?current_user.zipcode %> 
                <% if p.category.eql? 'Agency'%>
                  <tr>
                    <th>Title: <%= p.title %>
                    </br>
                    <%if User.find(p.user_id).name.eql?current_user.name %>
