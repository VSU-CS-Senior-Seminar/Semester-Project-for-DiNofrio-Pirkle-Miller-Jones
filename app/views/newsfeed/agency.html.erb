                    <% end %></th>
                  </tr>
            </thead>
            <tbody> 
              <tr>
                <td>
                  User: <%= User.find(p.user_id).name %>
                  </br>
                  Category: <%= p.category %>
                  </br></br>
                  <p><%= p.content %></p>
                  </br>
                  <a style="color:black;"><%= link_to "Comments", post_comments_path(p) %>: <%= p.comments.size %> </a>
                </td>
              </tr>
              <% end %>
              <% end %>
                <% end %>
            </tbody>
